<script>
    $(document).ready(function () {
        var val = '@HttpContextAccessor.HttpContext.Session.GetString("current_sys")';
        $("#CURRENT_SYSTEM").append(" " + Sys_Desc(val));
        $('#hospital_drop_down').css({ display: 'none' });
        $('#cluster_list').css({ display: 'none' });
        $('#cluster_include_all_list').css({ display: 'none' });
        $('#hosp_cluster_list').css({ display: 'none' });
        $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        showmenu();
        setTimeout(function () { hidemenu();}, 0.1);
    });

    // Show hide drop down list
    $('li').click(function () {
        var name = ''
        if ($(this).attr('name')!= ''&& $(this).attr('name')!= null && $(this).attr('name')!= undefined) {
            name = $(this).attr('name').toString();
        }
        if (name == '') {
            $('#hospital_drop_down').css({ display: 'none' });
            $('#cluster_list').css({ display: 'none' });
            $('#cluster_include_all_list').css({ display: 'none' });
            $('#hosp_cluster_list').css({ display: 'none' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        } else if (name == 'hospital_drop_down_control') {
            $('#hospital_drop_down').css({ display: 'block' });
            $('#cluster_list').css({ display: 'none' });
            $('#cluster_include_all_list').css({ display: 'none' });
            $('#hosp_cluster_list').css({ display: 'none' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        } else if (name == 'cluster_list_control') {
            $('#hospital_drop_down').css({ display: 'none' });
            $('#cluster_list').css({ display: 'block' });
            $('#cluster_include_all_list').css({ display: 'none' });
            $('#hosp_cluster_list').css({ display: 'none' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        } else if (name == 'cluster_include_all_list_control') {
            $('#hospital_drop_down').css({ display: 'none' });
            $('#cluster_list').css({ display: 'none' });
            $('#cluster_include_all_list').css({ display: 'block' });
            $('#hosp_cluster_list').css({ display: 'none' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        } else if (name == 'hosp_cluster_list_control') {
            $('#hospital_drop_down').css({ display: 'none' });
            $('#cluster_list').css({ display: 'none' });
            $('#cluster_include_all_list').css({ display: 'none' });
            $('#hosp_cluster_list').css({ display: 'block' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'none' });
        } else if (name == 'hosp_cluster_exclude_all_list_control') {
            $('#hospital_drop_down').css({ display: 'none' });
            $('#cluster_list').css({ display: 'none' });
            $('#cluster_include_all_list').css({ display: 'none' });
            $('#hosp_cluster_list').css({ display: 'none' });
            $('#hosp_cluster_exclude_all_list').css({ display: 'block' });
        };

    });

    $('#input').mouseover(function () {
        showmenu();
    });

    //$('#sidebar-menu').mouseleave(function () {
    $('#input').click(function () {
        hidemenu();
    });

    function hidemenu() {
        $('#left-sidebar-menu-asoi').css({ display: 'none' });
        $('#left-sidebar-menu-rentalincome').css({ display: 'none' });
        $('#left-sidebar-menu-designateddonation').css({ display: 'none' });
        $('#left-sidebar-menu-capitalsubvention').css({ display: 'none' });
        $('#left-sidebar-menu-stock').css({ display: 'none' });
        $('#left-sidebar-menu-annualcapitalcommitments').css({ display: 'none' });
        $('#left-sidebar-menu-listoffinancelease').css({ display: 'none' });
        $('#left-sidebar-menu-contingentliabilities').css({ display: 'none' });
        $('#left-sidebar-menu-bankdeposit').css({ display: 'none' });
        $('#left-sidebar-menu-bankrecon').css({ display: 'none' });
        $('#left-sidebar-menu-prepayment').css({ display: 'none' });
        $('#left-sidebar-menu-breakdownofcapitalprojects').css({ display: 'none' });
        $('#left-sidebar-menu-relatedpartytran').css({ display: 'none' });
        $('#left-sidebar-menu-relatedpartytranhacm').css({ display: 'none' });
        $('#left-sidebar-menu-donationrecieiptanexp').css({ display: 'none' });
        $('#left-sidebar-menu-finnacialvetting').css({ display: 'none' });
        $('#input .fa-angle-left').css('transform', 'rotate(' + 0 + 'deg)');
    };

    function showmenu() {
        $('#left-sidebar-menu-asoi').css({ display: 'block' });
        $('#left-sidebar-menu-rentalincome').css({ display: 'block' });
        $('#left-sidebar-menu-designateddonation').css({ display: 'block' });
        $('#left-sidebar-menu-capitalsubvention').css({ display: 'block' });
        $('#left-sidebar-menu-stock').css({ display: 'block' });
        $('#left-sidebar-menu-annualcapitalcommitments').css({ display: 'block' });
        $('#left-sidebar-menu-listoffinancelease').css({ display: 'block' });
        $('#left-sidebar-menu-contingentliabilities').css({ display: 'block' });
        $('#left-sidebar-menu-bankdeposit').css({ display: 'block' });
        $('#left-sidebar-menu-bankrecon').css({ display: 'block' });
        $('#left-sidebar-menu-prepayment').css({ display: 'block' });
        $('#left-sidebar-menu-breakdownofcapitalprojects').css({ display: 'block' });
        $('#left-sidebar-menu-relatedpartytran').css({ display: 'block' });
        $('#left-sidebar-menu-relatedpartytranhacm').css({ display: 'block' });
        $('#left-sidebar-menu-donationrecieiptanexp').css({ display: 'block' });
        $('#left-sidebar-menu-finnacialvetting').css({ display: 'block' });
        $('#input .fa-angle-left').css('transform', 'rotate(' + 270 + 'deg)');
    };

    function addInst_Code(element, list_id) {
        list_name = "#" + list_id;
        $(element).attr('href', function () {
            return this.href + '?Inst_Code=' + $(list_name).val();
        });
    }
    function addInst_CodeWithParam(element, list_id) {
        list_name = "#" + list_id;
        $(element).attr('href', function () {
            return this.href + '&Inst_Code=' + $(list_name).val();
        });
    }
    function addHosp_CodeWithParam(element, list_id) {
        list_name = "#" + list_id;
        $(element).attr('href', function () {
            return this.href + '&hosp_Code=' + $(list_name).val();
        });
    }
    $(document).ready(function () {
        var url = window.location;
        var element = $('ul.sidebar-menu a').filter(function () {
            return this.href == url || url.href.indexOf(this.href) == 0;
        }).parent().addClass('active');
        if (element.is('li')) {
            element.addClass('active').parent().parent('li').addClass('active')
        }
    });

</script>